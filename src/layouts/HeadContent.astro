---
import "../styles/global.css";
const props = Astro.props;
---

<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta name="description" content={props.default_description} />
<title>{props.site_name}</title>
<link rel="icon" href=`${props.Favicon.url}` />
<link rel="preconnect" href="https://use.typekit.net" />
<link rel="stylesheet" href="https://use.typekit.net/deh7bqm.css" media="print" onload="this.media='all'" />
<noscript><link rel="stylesheet" href="https://use.typekit.net/deh7bqm.css" /></noscript>
